<li>
  <div class="px-4 py-4 flex items-center sm:px-6">
    <div class="min-w-0 flex-1 sm:flex sm:items-center sm:justify-between">
      <div>

        <div class="flex text-sm font-medium text-cyan-600 truncate">
          <a href="#">
            <!-- Heroicon name: solid/star -->
            <svg class="flex-shrink-0 mr-1.5 h-5 w-5 text-gray-400" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" aria-hidden="true">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11.049 2.927c.3-.921 1.603-.921 1.902 0l1.519 4.674a1 1 0 00.95.69h4.915c.969 0 1.371 1.24.588 1.81l-3.976 2.888a1 1 0 00-.363 1.118l1.518 4.674c.3.922-.755 1.688-1.538 1.118l-3.976-2.888a1 1 0 00-1.176 0l-3.976 2.888c-.783.57-1.838-.197-1.538-1.118l1.518-4.674a1 1 0 00-.363-1.118l-3.976-2.888c-.784-.57-.38-1.81.588-1.81h4.914a1 1 0 00.951-.69l1.519-4.674z" />
            </svg>
          </a>

          <p>
          <%= schema.name %>
          </p>
          <p class="ml-1 font-normal text-gray-500">
          in <%= schema.service.name %>
          </p>
        </div>
        <div class="mt-2 flex">
          <div class="px-7 flex items-center text-sm text-gray-600">
            <p>
            <%= schema.description %>
            </p>
          </div>
        </div>
        <div class="mt-2 flex">
          <div class="px-7 flex items-center text-sm text-gray-400">
            <p>
            Last Updated on:
            <time datetime="2020-01-07"><%= I18n.l(schema.updated_at, format: :long) %></time>
            </p>
          </div>
        </div>
      </div>
      <div class="flex mt-0 right-0">

        <div class="relative mt-4 flex-shrink-0 sm:mt-0">
          <div class="overflow-hidden">
            <%= stakeholders(schema.stakeholders) %>
          </div>
        </div>
        <div class="relative mt-1 ml-2 flex-shrink-0">
          <a href="/schemas/<%= schema.id %>">
            <!-- Heroicon name: solid/chevron-right -->
            <svg class="h-5 w-5 text-gray-400" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
              <path fill-rule="evenodd" d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z" clip-rule="evenodd" />
            </svg>
          </a>
        </div>

      </div>
    </div>
  </div>
</li>
